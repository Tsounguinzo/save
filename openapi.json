      "get": {
        "tags": [
          "credits"
        ],
        "summary": "Get remaining credits",
        "description": "Retrieves the remaining credits for the authenticated user",
        "responses": {
          "200": {
            "description": "Contains creditsLeft field",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": true
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
